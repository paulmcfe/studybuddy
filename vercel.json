{
  "version": 2,
  "builds": [
    {
      "src": "v2-rag-from-scratch/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/v2-rag-from-scratch/api/index.py" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "bash scripts/should-deploy.sh"
}
