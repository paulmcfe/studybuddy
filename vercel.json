{
  "version": 2,
  "builds": [
    {
      "src": "v5-multi-agent/api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "v5-multi-agent/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "v5-multi-agent/api/index.py": {
      "includeFiles": "v5-multi-agent/documents/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/v5-multi-agent/api/index.py" },
    { "src": "/(.*)", "dest": "/v5-multi-agent/frontend/$1" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "ignoreCommand": "bash scripts/should-deploy.sh"
}
